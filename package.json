{
  "name": "react-router-in-gas",
  "version": "1.0.0",
  "scripts": {
    "glogin": "clasp login",
    "gcreate": "clasp create --title \"New App\" --rootDir ./dist",
    "gpush": "clasp push",
    "gpull": "clasp pull",
    "gstart": "clasp push --watch",
    "glogout": "clasp logout",
    "start": "parcel src/index.html --dist-dir ./dist",
    "build": "parcel build src/index.html --dist-dir ./dist"
  },
  "author": "spanifer",
  "license": "ISC",
  "main": "index.js",
  "keywords": [],
  "description": "Boilerplate of React and React Router in Google Apps Script",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@parcel/transformer-sass": "^2.9.3",
    "@types/google-apps-script": "^1.0.67",
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "typescript": "^5.2.2"
  },
  "typeAcquisition": {
    "enable": true,
    "include": [
      "google-apps-script"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  }
}
